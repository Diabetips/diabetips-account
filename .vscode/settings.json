{
    "fileHeaderComment.parameter": {
        "*": {
            "prefixBeg": "/*!",
            "prefixMid": "**",
            "prefixEnd": "*/"
        }
    },
    "fileHeaderComment.template": {
        "*": [
            "${prefixBeg}",
            "${prefixMid} Copyright 2019-${year} Diabetips",
            "${prefixMid}",
            "${prefixMid} All rights reserved",
            "${prefixMid}",
            "${prefixMid} Created by ${author} on ${date}",
            "${prefixEnd}"
        ]
    },
    "files.exclude": {
        ".git": true,
        "**/build": true,
        "**/dist": true,
        "**/logs": true,
        "**/node_modules": true
    }
}
